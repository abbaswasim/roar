#include "roar_version.hpp"

std::string roar_version()
{
	return "@RoarEngine_VERSION@";
}

unsigned roar_version_major()
{
	return @RoarEngine_VERSION_MAJOR@;
}

unsigned roar_version_minor()
{
	return @RoarEngine_VERSION_MINOR@+0;
}

unsigned roar_version_patch()
{
	return @RoarEngine_VERSION_PATCH@+0;
}

unsigned roar_version_tweak()
{
	return @RoarEngine_VERSION_TWEAK@+0;
}
